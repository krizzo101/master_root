{
  "dashboard": {
    "id": null,
    "title": "Celery Task Performance",
    "tags": ["autonomous-factory", "celery", "performance"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Active Workers",
        "type": "stat",
        "targets": [
          {
            "expr": "celery_workers_active",
            "legendFormat": "Active Workers"
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 0},
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 2},
                {"color": "green", "value": 3}
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "Tasks per Second",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(celery_tasks_total[5m])",
            "legendFormat": "Tasks/sec"
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 0},
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 10},
                {"color": "red", "value": 50}
              ]
            }
          }
        }
      },
      {
        "id": 3,
        "title": "Task Success Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(celery_tasks_total{status=\"SUCCESS\"}[5m]) / rate(celery_tasks_total[5m]) * 100",
            "legendFormat": "Success Rate %"
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 0},
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 80},
                {"color": "green", "value": 95}
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "title": "Queue Length",
        "type": "stat",
        "targets": [
          {
            "expr": "celery_queue_length",
            "legendFormat": "Queue Length"
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 0},
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 10},
                {"color": "red", "value": 50}
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "title": "Task Execution Timeline",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(celery_tasks_total[5m])",
            "legendFormat": "Tasks/sec"
          },
          {
            "expr": "rate(celery_tasks_total{status=\"SUCCESS\"}[5m])",
            "legendFormat": "Successful Tasks/sec"
          },
          {
            "expr": "rate(celery_tasks_total{status=\"FAILURE\"}[5m])",
            "legendFormat": "Failed Tasks/sec"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 4},
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            }
          }
        }
      },
      {
        "id": 6,
        "title": "Queue Length by Queue",
        "type": "timeseries",
        "targets": [
          {
            "expr": "celery_queue_length{queue=\"default\"}",
            "legendFormat": "Default Queue"
          },
          {
            "expr": "celery_queue_length{queue=\"io\"}",
            "legendFormat": "IO Queue"
          },
          {
            "expr": "celery_queue_length{queue=\"heavy\"}",
            "legendFormat": "Heavy Queue"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 4},
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            }
          }
        }
      },
      {
        "id": 7,
        "title": "Task Duration by Agent Type",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(autonomous_factory_task_duration_seconds_bucket{agent_type=\"planner\"}[5m]))",
            "legendFormat": "Planner (p95)"
          },
          {
            "expr": "histogram_quantile(0.95, rate(autonomous_factory_task_duration_seconds_bucket{agent_type=\"specifier\"}[5m]))",
            "legendFormat": "Specifier (p95)"
          },
          {
            "expr": "histogram_quantile(0.95, rate(autonomous_factory_task_duration_seconds_bucket{agent_type=\"architect\"}[5m]))",
            "legendFormat": "Architect (p95)"
          },
          {
            "expr": "histogram_quantile(0.95, rate(autonomous_factory_task_duration_seconds_bucket{agent_type=\"coder\"}[5m]))",
            "legendFormat": "Coder (p95)"
          },
          {
            "expr": "histogram_quantile(0.95, rate(autonomous_factory_task_duration_seconds_bucket{agent_type=\"tester\"}[5m]))",
            "legendFormat": "Tester (p95)"
          },
          {
            "expr": "histogram_quantile(0.95, rate(autonomous_factory_task_duration_seconds_bucket{agent_type=\"critic\"}[5m]))",
            "legendFormat": "Critic (p95)"
          }
        ],
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 12},
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "color": {
              "mode": "palette-classic"
            }
          }
        }
      },
      {
        "id": 8,
        "title": "Worker Status",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=\"celery-workers\"}",
            "legendFormat": "Worker Status"
          }
        ],
        "gridPos": {"h": 4, "w": 24, "x": 0, "y": 20},
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "green", "value": 1}
              ]
            }
          }
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "30s"
  }
}
