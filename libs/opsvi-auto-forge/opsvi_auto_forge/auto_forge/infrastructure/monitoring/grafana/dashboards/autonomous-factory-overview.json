{
  "dashboard": {
    "id": null,
    "title": "Autonomous Factory - System Overview",
    "tags": ["autonomous-factory", "overview", "performance"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Pipeline Execution Status",
        "type": "stat",
        "targets": [
          {
            "expr": "autonomous_factory_pipeline_runs_total",
            "legendFormat": "Total Pipeline Runs"
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 0},
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 10},
                {"color": "red", "value": 50}
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "Active Pipeline Runs",
        "type": "stat",
        "targets": [
          {
            "expr": "autonomous_factory_pipeline_runs_active",
            "legendFormat": "Active Runs"
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 0},
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 5},
                {"color": "red", "value": 10}
              ]
            }
          }
        }
      },
      {
        "id": 3,
        "title": "Task Success Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(autonomous_factory_tasks_completed_total{status=\"success\"}[5m]) / rate(autonomous_factory_tasks_completed_total[5m]) * 100",
            "legendFormat": "Success Rate %"
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 0},
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 80},
                {"color": "green", "value": 95}
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "title": "Average Task Duration",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(autonomous_factory_task_duration_seconds_sum[5m]) / rate(autonomous_factory_task_duration_seconds_count[5m])",
            "legendFormat": "Avg Duration (s)"
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 0},
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 30},
                {"color": "red", "value": 60}
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "title": "Pipeline Execution Timeline",
        "type": "timeseries",
        "targets": [
          {
            "expr": "autonomous_factory_pipeline_runs_total",
            "legendFormat": "Total Runs"
          },
          {
            "expr": "autonomous_factory_pipeline_runs_active",
            "legendFormat": "Active Runs"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 4},
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            }
          }
        }
      },
      {
        "id": 6,
        "title": "Task Completion Rate",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(autonomous_factory_tasks_completed_total[5m])",
            "legendFormat": "Tasks/sec"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 4},
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            }
          }
        }
      },
      {
        "id": 7,
        "title": "Agent Performance by Type",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(autonomous_factory_agent_tasks_total{agent_type=\"planner\"}[5m])",
            "legendFormat": "Planner"
          },
          {
            "expr": "rate(autonomous_factory_agent_tasks_total{agent_type=\"specifier\"}[5m])",
            "legendFormat": "Specifier"
          },
          {
            "expr": "rate(autonomous_factory_agent_tasks_total{agent_type=\"architect\"}[5m])",
            "legendFormat": "Architect"
          },
          {
            "expr": "rate(autonomous_factory_agent_tasks_total{agent_type=\"coder\"}[5m])",
            "legendFormat": "Coder"
          },
          {
            "expr": "rate(autonomous_factory_agent_tasks_total{agent_type=\"tester\"}[5m])",
            "legendFormat": "Tester"
          },
          {
            "expr": "rate(autonomous_factory_agent_tasks_total{agent_type=\"critic\"}[5m])",
            "legendFormat": "Critic"
          }
        ],
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 12},
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            }
          }
        }
      },
      {
        "id": 8,
        "title": "System Health Status",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=\"api\"}",
            "legendFormat": "API"
          },
          {
            "expr": "up{job=\"celery-workers\"}",
            "legendFormat": "Celery Workers"
          },
          {
            "expr": "up{job=\"neo4j\"}",
            "legendFormat": "Neo4j"
          },
          {
            "expr": "up{job=\"redis\"}",
            "legendFormat": "Redis"
          }
        ],
        "gridPos": {"h": 4, "w": 24, "x": 0, "y": 20},
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "green", "value": 1}
              ]
            }
          }
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "30s"
  }
}