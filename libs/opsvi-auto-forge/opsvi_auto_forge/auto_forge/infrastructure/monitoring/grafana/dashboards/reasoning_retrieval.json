        {
          "expr": "sum(rate(dk_verifier_disagreement_total[5m])) by (model)",
          "legendFormat": "{{model}}"
        }
      ],
      "id": 2,
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      }
    },
    {
      "type": "graph",
      "title": "Retrieval Latency p90",
      "targets": [
        {
          "expr": "histogram_quantile(0.90, sum(rate(ro_retrieval_latency_ms_bucket[5m])) by (le, retriever))",
